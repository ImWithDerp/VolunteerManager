<myTypes:BaseMenu x:Class="VolunteerManager.Menus.MainMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VolunteerManager.Menus"
             xmlns:myTypes="clr-namespace:VolunteerManager.Menus"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Label x:Name="title" Content="Volunteer Manager" Margin="0,10,0,0" FontSize="15" FontWeight="Heavy"  VerticalAlignment="Top" HorizontalAlignment="Center"/>
        <Label x:Name="version" Content="Version Alpha" Margin="0,40,0,0" FontSize="10" VerticalAlignment="Top" HorizontalAlignment="Center"/>
        <Button x:Name="signinButton" Content="Volunteer Sign-In Page" HorizontalAlignment="Center" Margin="0,120,0,0" VerticalAlignment="Top"  Width="150" Click="signinButton_Click"/>
        <Button x:Name="adminButton" Content="Administration Page" HorizontalAlignment="Center" Margin="0,160,0,0" VerticalAlignment="Top"  Width="150" Click="adminButton_Click"/>
    </Grid>
</myTypes:BaseMenu>
